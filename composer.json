{
    "name": "pcont/my-mvc",
    "description": "My own MVC framework project for learning purposes",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "pedro-dim",
            "email": "pcontato19@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.25",
        "mockery/mockery": "^1.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "require": {
        "intervention/image": "^2.7",
        "doctrine/orm": "^2.17",
        "pedrodim/camus": "dev-main",
        "twig/twig": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "fakerphp/faker": "^1.23",
        "pecee/simple-router": "^5.4.1.4",
        "nette/di": "^3.1",
        "nesbot/carbon": "^2.72",
        "firebase/php-jwt": "^6.10",
        "predis/predis": "^2.2",
        "nunomaduro/collision": "^7.10",
        "psr/http-server-middleware": "^1.0",
        "php-di/php-di": "^7.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./repositories/camus"
        }
    ]
}
